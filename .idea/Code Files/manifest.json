{
  "manifest_version": 3,
  "name": "StudyBR",
  "version": "1.0",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["https://generativelanguage.googleapis.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "StudyBR.png",
      "48": "StudyBR.png",
      "128": "StudyBR.png"
    }
  },
  "icons": {
    "16": "StudyBR.png",
    "48": "StudyBR.png",
    "128": "StudyBR.png"
  }
}
